<h1>Articles</h1>

<table>
    <thead>
        <tr>
            <th>title</th>
            <th>body</th>
            <th>placeholder</th>
        </tr>
    </thead>
    <tbody>
        <% @articles.each do |article|%>
        <tr>
            <td><%= article.title%></td>
            <td><%= article.body %></td>
            <td>placeholder</td>
        </tr>
        <%end%>
    </tbody>
</table>